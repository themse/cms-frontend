{
  "name": "cms-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "server:fake": "json-server --watch fake-server/store.json --port 3009",
    "eject": "react-scripts eject",
    "clean": "prettier --write '**/*.{js,css,md}'"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "husky": "^4.2.5",
    "lint-staged": "^10.1.7",
    "normalizr": "^3.6.0",
    "pure-react-carousel": "^1.27.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.1",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0",
    "yup": "^0.28.4"
  },
  "devDependencies": {
    "json-server": "^0.16.1",
    "prettier": "^2.0.5"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}